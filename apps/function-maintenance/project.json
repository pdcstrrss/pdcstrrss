{
  "name": "function-maintenance",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/function-maintenance/src",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "options": {
        "platform": "node",
        "target": "node18",
        "outputPath": "dist/hosting/netlify/functions",
        "tsConfig": "apps/function-maintenance/tsconfig.json",
        "main": "apps/function-maintenance/src/maintenance.ts",
        "external": ["@prisma/client"],
        "generatePackageJson": false,
        "thirdParty": true
      }
    },
    "package": {
      "executor": "@nx/esbuild:esbuild",
      "options": {
        "platform": "node",
        "target": "node18",
        "outputPath": "dist/hosting/netlify/functions",
        "tsConfig": "apps/function-maintenance/tsconfig.json",
        "main": "apps/function-maintenance/src/maintenance.ts",
        "external": ["@prisma/client"],
        "generatePackageJson": false,
        "thirdParty": true
      }
    },
    "dev": {
      "executor": "@nx/esbuild:esbuild",
      "options": {
        "platform": "node",
        "target": "node18",
        "outputPath": "dist/hosting/netlify/functions",
        "tsConfig": "apps/function-maintenance/tsconfig.json",
        "main": "apps/function-maintenance/src/maintenance.ts",
        "external": ["@prisma/client"],
        "generatePackageJson": false,
        "thirdParty": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/function-maintenance/**/*.ts"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vitest --run",
        "cwd": "apps/function-maintenance"
      }
    }
  },
  "tags": []
}
