{
  "root": "apps/web-remix",
  "sourceRoot": "apps/web-remix",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "cross-env NODE_ENV=development netlify dev"
          }
        ],
        "cwd": "apps/web-remix"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "netlify build"
          }
        ],
        "cwd": "apps/web-remix"
      }
    }
  }
}
