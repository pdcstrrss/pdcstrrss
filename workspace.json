{
  "version": 2,
  "projects": {
    "core": "libs/core",
    "database": "libs/database",
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-remix": {
      "root": "apps/web-remix",
      "sourceRoot": "apps/web-remix",
      "projectType": "application",
      "tags": [],
      "targets": {
        "dev": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "cross-env NODE_ENV=development netlify dev"
              }
            ],
            "cwd": "apps/web-remix"
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "remix build"
              }
            ],
            "cwd": "apps/web-remix"
          }
        }
      }
    },
    "web-remix-e2e": {
      "root": "apps/web-remix-e2e",
      "sourceRoot": "apps/web-remix-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx nx run web-remix:dev"
              },
              {
                "command": "npx cypress run -P apps/web-remix-e2e"
              }
            ],
            "readyWhen": "Run Finished"
          },
          "configurations": {
            "watch": {
              "commands": [
                {
                  "command": "npx nx run web-remix:dev"
                },
                {
                  "command": "npx cypress open -P apps/web-remix-e2e"
                }
              ],
              "parallel": true
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/web-remix-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["web-remix"]
    }
  }
}
